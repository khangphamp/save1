    "bootstrap": "5.1.3",
    "chart.js": "^4.4.0",
    "faker": "^5.5.3",
    "formik": "^2.2.9",
    "fs": "^0.0.1-security",
    "moment": "^2.29.4",
    "query-string": "^7.1.1",
    "react": "^18.1.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.0.0",
    "react-quill": "^2.0.0",
    "react-redux": "^7.2.6",
    "react-responsive-carousel": "^3.2.22",
    "react-router-dom": "^5.2.0",
    "react-scripts": "5.0.0",
    "react-select": "^5.2.2",
    "react-table": "^7.7.0",
    "react-toastify": "^8.1.1",
    "react-tsparticles": "^1.40.0",
    "reactstrap": "^9.0.1",
    "redux": "^4.1.2",
    "redux-saga": "^1.1.3",
